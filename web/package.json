{
	"name": "poseEstimationJs",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"@tensorflow/tfjs-converter": "0.1.1",
		"@tensorflow/tfjs-core": "0.6.1"
	},
	"scripts": {
		"watch": "NODE_ENV=development parcel --no-hmr --open index.html ",
		"build": "NODE_ENV=production parcel build index.html --no-minify --public-url /",
		"tfjs": "python 00saveWithModelWithMetaGraph.py;sh 01convertToTfjsModel.sh;sh 02copyFiles.sh",
		"clean": "rm -rf .cache dist node_modules images saved_model web_model yarn-error.log"
	},
	"devDependencies": {
		"babel-plugin-transform-runtime": "~6.23.0",
		"babel-polyfill": "~6.26.0",
		"babel-preset-env": "~1.6.1",
		"clang-format": "~1.2.2",
		"parcel-bundler": "~1.6.2"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"modules": false,
					"targets": {
						"browsers": [
							"> 1%",
							"last 3 versions",
							"ie >= 9",
							"ios >= 8",
							"android >= 4.2"
						]
					},
					"useBuiltIns": false
				}
			]
		],
		"plugins": [
			"transform-runtime"
		]
	}
}
